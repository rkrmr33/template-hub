          "format": "int32"
        },
        "http_code": {
          "type": "integer",
          "format": "int32"
        },
        "http_status": {
          "type": "string"
        },
        "message": {
          "type": "string"
        }
      }
    },
    "versionVersionResponse": {
      "type": "object",
      "title": "VersionMessage represents the version of the server",
      "properties": {
        "BuildDate": {
          "type": "string"
        },
        "Compiler": {
          "type": "string"
        },
        "GitCommit": {
          "type": "string"
        },
        "GoVersion": {
          "type": "string"
        },
        "Version": {
          "type": "string"
        }
      }
    }
  }
